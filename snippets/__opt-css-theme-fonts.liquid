<style>
@font-face{
	font-family:'Proxima Nova Semibold';
	font-style:normal;
	font-weight:400;
	src:local("Proxima Nova Semibold"),
	url('{{ 'ProximaNova-Semibold.woff' | asset_url }}') format("woff");
	font-display:swap
}

@font-face{
	font-display:swap;
	font-family:turbo;
	src:url(//cdn.shopify.com/s/files/1/0054/1137/3091/t/4/assets/turbo.eot?3106);
	src:url(//cdn.shopify.com/s/files/1/0054/1137/3091/t/4/assets/turbo.eot?3106) format("embedded-opentype"),
	url(//cdn.shopify.com/s/files/1/0054/1137/3091/t/4/assets/turbo.woff?3106) format("woff"),
	url(//cdn.shopify.com/s/files/1/0054/1137/3091/t/4/assets/turbo.ttf?3106) format("truetype"),
	url(//cdn.shopify.com/s/files/1/0054/1137/3091/t/4/assets/turbo.svg?3106) format("svg");
	font-weight:400;
	font-style:normal
}

@font-face{
	font-family:'Proxima Nova Regular';
	font-style:normal;
	font-weight:400;
	src:local("Proxima Nova Regular"),
	url('{{ 'ProximaNova-Regular.woff' | asset_url }}') format("woff");
	font-display:swap
}

@font-face{
	font-family:Roboto;
	font-display:swap
}

@font-face{
	font-family:Vollkorn;
	font-display:swap
}

@font-face{
	font-family:Montserrat;
	font-display:swap
}

</style>

{% comment %} <script src="https://ajax.googleapis.com/ajax/libs/webfont/1.6.26/webfont.js"></script>
<script>
	WebFont.load({
		google: {
			families: ['Vollkorn', 'Cabin', 'Roboto', 'Playfair Display', 'Montserrat']
		}
	});
</script> {% endcomment %}